{
	"name": "SinkIncrementalData",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"folder": {
			"name": "Incremental Copy datasets"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@CONCAT('Incremental-', pipeline().RunId, '.txt')",
					"type": "Expression"
				},
				"fileSystem": "outputfiles"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}